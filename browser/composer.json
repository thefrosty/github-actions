{
    "name": "thefrosty/github-actions-browser",
    "description": "",
    "type": "project",
    "license": "MIT",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["application"],
    "require": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.36.1",
        "nunomaduro/laravel-console-dusk": "^1.13"
    },
    "require-dev": {
        "laravel/pint": "^1.19",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    }
}
